  </connectionStrings>
  <appSettings>
    <add key="AllowInsecureHttp" value="true"/>
    <add key="EnableApiTestPage" value="true"/>
    <add key="Auth.TokenExpiryInDays" value="14"/>

    <!-- DO NOT REMOVE IN DEV OR TEST ENVIRONMENTS -->
